{
  "version": 2,
  "builds": [
    {
      "src": "apps/onlyconvert/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "apps/onlyconvert/dist" }
    }
  ],
  "routes": [
    { "src": "/robots.txt", "dest": "/apps/onlyconvert/dist/robots.txt" },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/apps/onlyconvert/dist/index.html" }
  ]
}
